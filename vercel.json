{
    "version": 2,
    "builds": [
        {
            "src": "/server/**",
            "use": "@vercel/node"
        },
        {
            "src": "/src/**",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "./build"
            }
        }
    ],
    "routes": [
        {
            "src": "/api/v1/(.*)",
            "dest": "/server/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/src/$1"
        }
    ]
}